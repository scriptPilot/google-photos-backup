  },
  "license": "MIT",
  "keywords": [
    "google",
    "photos",
    "downloader",
    "playwright",
    "download",
    "photo"
  ],
  "dependencies": {
    "exiftool-vendored": "^22.0.0",
    "move-file": "^3.1.0",
    "pino": "^8.14.1",
    "playwright": "^1.35.1",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "user-agents": "^1.1.391"
  },
  "devDependencies": {
    "standard": "^17.0.0"
  }
