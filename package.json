{
  "name": "google-photos-backup",
  "version": "1.2.0",
  "description": "Download photos from Google Photos using Playwright",
  "repository": "vikas5914/google-photos-backup",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "backup": "node index.js --headless=false",
    "cleanup": "node cleanup.js --headless=false",
    "cleanup-dev": "echo https://photos.google.com/photo/AF1QipPfNXp2NJW5Iro62C9VCWjcnPmn06Tw7_OzoPhg > .lastdone && node archive.js --headless=false"
  },
  "author": {
